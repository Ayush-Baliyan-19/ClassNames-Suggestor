{
  "name": "css-suggestor",
  "displayName": "Css-Suggestor",
  "description": "Suggests CSS classes from npm packages",
  "version": "0.0.3",
  "publisher": "AyushBaliyan",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:typescriptjsx",
    "onCommand:css-suggestor.setPackage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-suggestor.setPackage",
        "title": "CSS Suggestor: Set NPM Package"
      }
    ],
    "configuration": {
      "title": "CSS Suggestor",
      "properties": {
        "cssSuggestor.enableQuickSuggestions": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable quick suggestions for CSS classes"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}